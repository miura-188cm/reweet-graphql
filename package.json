{
  "name": "remeet-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:next":"next dev",
    "dev:next:db": "concurrently \"next dev\" \"npm run docker:dev:up\"",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "npx eslint . --fix",
    "codeGen": "npx graphql-codegen",
    "docker:ps": "docker ps",
    "docker:dev:up": "docker compose -f docker-compose.dev.yaml up -d db",
    "docker:dev:wait": "docker compose -f docker-compose.dev.yaml wait db",
    "docker:dev:down": "docker compose -f docker-compose.dev.yaml down -v",
    "prepare": "husky",
    "prettier": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,css,html}\"",
    "prisma:migration:dev": "dotenv -e .env.local -- npx prisma migrate dev",
    "prisma:migration:test": "dotenv -e .env.test -- npx prisma migrate dev",
    "prisma:seed": "prisma db seed",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml,css,scss,html}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@apollo/client": "^4.1.4",
    "@apollo/server": "^5.4.0",
    "@auth/prisma-adapter": "^2.11.0",
    "@conform-to/react": "^1.13.1",
    "@conform-to/zod": "^1.13.1",
    "@graphql-codegen/cli": "^6.1.1",
    "@graphql-codegen/typescript": "^5.0.7",
    "@graphql-codegen/typescript-operations": "^5.0.7",
    "@graphql-codegen/typescript-react-apollo": "^4.4.0",
    "@graphql-codegen/typescript-resolvers": "^5.1.5",
    "@hookform/resolvers": "^5.2.2",
    "@prisma/client": "^6.16.3",
    "@prisma/extension-accelerate": "^2.0.2",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@vercel/analytics": "^1.6.1",
    "@vercel/speed-insights": "^1.3.1",
    "bcrypt": "^6.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "graphql": "^16.12.0",
    "lucide-react": "^0.546.0",
    "next": "^15.5.9",
    "next-auth": "^5.0.0-beta.29",
    "next-themes": "^0.4.6",
    "react": "19.1.0",
    "react-day-picker": "^9.12.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.68.0",
